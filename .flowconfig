[libs]
flow-typed

[options]
include_warnings=true
esproposal.optional_chaining=enable
esproposal.decorators=ignore
module.file_ext=.vue
module.file_ext=.ts
module.file_ext=.tsx
module.file_ext=.js
module.file_ext=.jsx
module.file_ext=.scss
module.file_ext=.css
module.name_mapper='^@/\(.*\)$' -> '<PROJECT_ROOT>/src/\1'

[ignore]
./node_modules/.*
./coverage/.*
./dist/.*
./public/.*

[include]
